{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Documentation",
  "colors": {
    "primary": "#007FFF",
    "light": "#007FFF",
    "dark": "#006AD4"
  },
  "icons": {
    "library": "lucide"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["index", "quickstart", "dashboard", "api-keys"]
          },

          {
            "group": "Integrations",
            "icon": "network",
            "pages": [
              "integrations/overview",
              "integrations/webhooks",
              "integrations/zapier",
              "integrations/mcp"
            ]
          },
          {
            "group": "Developers",
            "icon": "terminal",
            "pages": [
              "builders/submit-data",
              "builders/get-profile",
              "builders/search-profiles"
            ]
          },
          {
            "group": "SDKs",
            "icon": "code",
            "pages": ["sdks/nodejs"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication"
            ]
          },
          {
            "group": "Search API",
            "icon": "sparkles",
            "pages": ["api-reference/search/get"]
          },
          {
            "group": "Profile API",
            "pages": [
              "api-reference/profile/get",
              "api-reference/profile/post",
              "api-reference/profile/delete"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "href": "https://getprofile-ai.com",
          "icon": "globe"
        },
        {
          "anchor": "Blog",
          "href": "https://getprofile-ai.com/blog",
          "icon": "rss"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo-light.svg",
    "dark": "/logo/logo-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/getprofile",
        "icon": "github"
      },
      {
        "label": "Support",
        "href": "mailto:support@getprofile-ai.com",
        "icon": "mail"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "http://getprofile-ai.com/dashboard"
    }
  },
  "footer": {
    "socials": {
      "website": "https://getprofile-ai.com",
      "linkedin": "https://www.linkedin.com/company/getprofile-ai",
      "x": "https://x.com/GetProfileAI",
      "facebook": "https://www.facebook.com/getprofileai",
      "github": "https://github.com/getprofile"
    },
    "links": [
      {
        "header": "Legal",
        "items": [
          {
            "label": "Privacy Policy",
            "href": "https://getprofile-ai.com/privacy"
          },
          {
            "label": "Terms of Service",
            "href": "https://getprofile-ai.com/terms"
          }
        ]
      }
    ]
  }
}
